<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<!-- This file must be present with our jar, even if it is empty. Otherwise, 
		our Java annotations won't get scanned! -->
<managed-bean>
<managed-bean-name>insuranceEjbImpl</managed-bean-name>
<managed-bean-class>com.java.ejb.InsuranceEjbImpl</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>policyImpl</managed-bean-name>
   <managed-bean-class>com.java.ejb.CustomerPolicyDaoImpl</managed-bean-class>
   <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>planImpl</managed-bean-name>
   <managed-bean-class>com.java.ejb.InsurancePlanDaoImpl</managed-bean-class>
   <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>insurancePaginationDao</managed-bean-name>
<managed-bean-class>com.java.ejb.InsurancePaginationBean</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>customerImpl</managed-bean-name>
<managed-bean-class>com.java.ejb.CustomerDetailsDaoImpl</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>customerAuthImpl</managed-bean-name>
<managed-bean-class>com.java.ejb.CustomerAuthorizationDaoImpl</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>customer</managed-bean-name>
<managed-bean-class>com.java.ejb.CustomerDetails</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>policy</managed-bean-name>
   <managed-bean-class>com.java.ejb.CustomerPolicy</managed-bean-class>
   <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>auth</managed-bean-name>
<managed-bean-class>com.java.ejb.CustomerAuthorization</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
</faces-config>